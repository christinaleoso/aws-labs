{
  "Version": "2012-10-17",
  "Id": "ExampleS3ReadOnlyForLogs",
  "Statement": [
    {
      "Sid": "AllowListAndGetOnLogsBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::corp-logs"
    },
    {
      "Sid": "AllowReadObjectsInLogsBucket",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::corp-logs/*"
    }
  ]
}
